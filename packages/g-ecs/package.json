{
  "name": "@antv/g-ecs",
  "version": "0.1.0",
  "private": true,
  "description": "A simple ECS implement",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "package.json",
    "esm",
    "lib",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {    
    "build": "npm run clean && run-p build:*",
    "build:esm": "tsc -p tsconfig.json --target ES5 --module ESNext --outDir esm",
    "build:cjs": "tsc -p tsconfig.json --target ES5 --module commonjs --outDir lib",
    "clean": "rm -rf esm lib dist",
    "coverage": "npm run coverage-generator && npm run coverage-viewer",
    "test": "yarn jest --coverage",
    "tsc": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antvis/util.git"
  },
  "keywords": [
    "util",
    "antv",
    "g"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "https://github.com/orgs/antvis/people",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antvis/util/issues"
  },
  "devDependencies": {
    "@types/jest": "^25.0.0",
    "@types/lodash": "^4.14.138",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "homepage": "https://github.com/antvis/util#readme",
  "dependencies": {
    "eventemitter3": "^4.0.0",
    "inversify": "^5.0.1",
    "lodash": "^4.17.15",
    "reflect-metadata": "^0.1.13"
  }
}
