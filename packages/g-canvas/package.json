{
  "name": "@antv/g-canvas",
  "version": "0.1.0",
  "private": true,
  "description": "A renderer implemented by Canvas 2D API",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "unpkg": "dist/g.min.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "package.json",
    "esm",
    "lib",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf esm lib dist",
    "build": "run-p build:*",
    "build:cjs": "BABEL_ENV=cjs babel src --root-mode upward --out-dir lib --source-maps --extensions .ts,.tsx --delete-dir-on-start",
    "build:esm": "BABEL_ENV=esm babel src --root-mode upward --out-dir esm --source-maps --extensions .ts,.tsx --delete-dir-on-start",
    "build:umd": "webpack --config webpack.config.js --mode production",
    "watch": "run-p watch:*",
    "watch:cjs": "BABEL_ENV=cjs babel src --watch --root-mode upward --out-dir lib --source-maps --extensions .ts,.tsx --delete-dir-on-start",
    "watch:esm": "BABEL_ENV=esm babel src --watch --root-mode upward --out-dir esm --source-maps --extensions .ts,.tsx --delete-dir-on-start",
    "coverage": "npm run coverage-generator && npm run coverage-viewer",
    "test": "yarn jest --coverage",
    "tsc": "run-p tsc:*",
    "tsc:esm": "tsc --project tsconfig.json --declarationDir esm",
    "tsc:lib": "tsc --project tsconfig.json --declarationDir lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antvis/g.git"
  },
  "keywords": [
    "util",
    "antv",
    "g"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "https://github.com/orgs/antvis/people",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antvis/g/issues"
  },
  "devDependencies": {
    "@types/jest": "^25.0.0",
    "@types/gl-matrix": "^2.4.5",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "homepage": "https://github.com/antvis/g#readme",
  "dependencies": {
    "@antv/g": "^0.1.0",
    "@antv/util": "^2.0.13",
    "@antv/g-plugin-canvas-renderer": "^0.1.0",
    "@antv/g-plugin-dom-interaction": "^0.1.0",
    "@antv/g-plugin-canvas-picker": "^0.1.0"
  }
}
