{
  "name": "@antv/g-webgl",
  "version": "0.1.0",
  "private": true,
  "description": "A renderer implemented by WebGL1",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "unpkg": "dist/g.min.js",
  "types": "lib/index.d.ts",
  "files": [
    "package.json",
    "esm",
    "lib",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {    
    "clean": "rm -rf esm lib dist",
    "build": "run-p build:*",
    "build:cjs": "BABEL_ENV=cjs babel src --root-mode upward --out-dir lib --source-maps --extensions .ts,.tsx --delete-dir-on-start",
    "build:esm": "BABEL_ENV=esm babel src --root-mode upward --out-dir esm --source-maps --extensions .ts,.tsx --delete-dir-on-start",
    "build:umd": "webpack --config webpack.config.js --mode production",
    "watch": "BABEL_ENV=esm babel src --watch --root-mode upward --out-dir esm --source-maps --extensions .ts,.tsx --delete-dir-on-start",
    "coverage": "npm run coverage-generator && npm run coverage-viewer",
    "test": "yarn jest --coverage",
    "tsc": "run-p tsc:*",
    "tsc:esm": "tsc --project tsconfig.json --declarationDir esm",
    "tsc:lib": "tsc --project tsconfig.json --declarationDir lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antvis/util.git"
  },
  "keywords": [
    "util",
    "antv",
    "g"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "https://github.com/orgs/antvis/people",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antvis/util/issues"
  },
  "devDependencies": {
    "@types/jest": "^25.0.0",
    "@types/gl-matrix": "^2.4.5",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "homepage": "https://github.com/antvis/util#readme",
  "dependencies": {
    "@antv/g-ecs": "^0.1.0",
    "@antv/g-core": "^0.1.0",
    "@antv/util": "^2.0.13",
    "@webgpu/types": "^0.0.31",
    "d3-color": "^1.4.0",
    "eventemitter3": "^4.0.0",
    "inversify": "^5.0.1",
    "regl": "^1.3.11",
    "reflect-metadata": "^0.1.13"
  }
}
