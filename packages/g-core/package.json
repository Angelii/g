{
  "name": "@antv/g-core",
  "version": "0.1.0",
  "private": true,
  "description": "A core module including ECS and DI container.",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "unpkg": "dist/g.min.js",
  "types": "lib/index.d.ts",
  "files": [
    "package.json",
    "esm",
    "lib",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {    
    "clean": "rm -rf esm lib dist",
    "build": "run-p build:*",
    "build:cjs": "BABEL_ENV=cjs babel src --root-mode upward --out-dir lib --source-maps --extensions .ts,.tsx --delete-dir-on-start",
    "build:esm": "BABEL_ENV=esm babel src --root-mode upward --out-dir esm --source-maps --extensions .ts,.tsx --delete-dir-on-start",
    "watch": "BABEL_ENV=esm babel src --watch --root-mode upward --out-dir esm --source-maps --extensions .ts,.tsx --delete-dir-on-start",
    "coverage": "npm run coverage-generator && npm run coverage-viewer",
    "test": "yarn jest --coverage",
    "tsc": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antvis/util.git"
  },
  "keywords": [
    "util",
    "antv",
    "g"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "https://github.com/orgs/antvis/people",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antvis/util/issues"
  },
  "devDependencies": {
    "@types/jest": "^25.0.0",
    "@types/gl-matrix": "^2.4.5",
    "@types/lodash": "^4.14.138",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "homepage": "https://github.com/antvis/util#readme",
  "dependencies": {
    "@antv/g-ecs": "^0.1.0",
    "eventemitter3": "^4.0.0",
    "gl-matrix": "^3.1.0",
    "inversify": "^5.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "lodash-es": "^4.17.15",
    "reflect-metadata": "^0.1.13"
  }
}
